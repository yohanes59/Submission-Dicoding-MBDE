{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "class-methods-use-this": 0,
        "camelcase": 0,
        "max-len": 0
    }
}
